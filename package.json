{
	"name": "obsidian-customize-discordrpc",
	"version": "1.0.0",
	"description": "Customizable Discord Rich Presence for Obsidian - Shows vault name, note name, and time spent",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ["obsidian", "discord", "rpc", "rich-presence", "productivity"],
	"author": "Ray-kong",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"discord-rpc": "^4.0.1",
		"dotenv": "^17.2.2"
	}
}
